<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:views = "clr-namespace:HENRY.View"
             xmlns:ViewModel="clr-namespace:HENRY.ViewModel" x:Class="HENRY.View.MasterView"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">

    <Grid>
        <views:UserView x:Name="UserViewControl" Loaded ="UserViewControl_Loaded" Visibility="Collapsed"/>
        <views:DevView x:Name="DevViewControl" Loaded ="DevViewControl_Loaded"/>
    </Grid>
</UserControl>
