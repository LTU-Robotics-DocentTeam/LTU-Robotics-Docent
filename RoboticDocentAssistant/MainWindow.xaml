<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local = "clr-namespace:HENRY"
        xmlns:views = "clr-namespace:HENRY.View"
        xmlns:ViewModel="clr-namespace:HENRY.ViewModel" x:Name="HENRY_Window" x:Class="HENRY.MainWindow"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Border Background="#FFED6E10" Visibility="{Binding UserVisibility, Mode=OneWay}">
            <views:UserView x:Name="UserViewControl" Loaded ="UserViewControl_Loaded"  />
        </Border>
        <Border Background="#FF0F86C2" Visibility="{Binding DevVisibility, Mode=OneWay}">
            <views:DevView x:Name="DevViewControl" Loaded ="DevViewControl_Loaded" />
        </Border>
        <Button x:Name="button1" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Click Me" Click="button1_Click"></Button>
        
    </Grid>
</Window>
